{
  "name": "matches-shape",
  "version": "1.2.1",
  "description": "A tool for testing whether an object is of a given shape",
  "author": "Sasha Klein",
  "repository": {
    "type": "git",
    "url": "https://github.com/sashafklein/shape.git"
  },
  "license": "MIT",
  "main": "lib/Shape.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": "6.2.x"
  },
  "scripts": {
    "test": "ava test --verbose",
    "prepublish": "npm run build",
    "build": "babel --presets es2015 -d lib/ src/"
  },
  "ava": {
    "files": [
      "test/**/*Test.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "ava-describe": "1.0.0",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "lodash": "^4.13.1"
  }
}
